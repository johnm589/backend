<h1>Listing profiles</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Title</th>
      <th>User</th>
      <th>Skill</th>
      <th>Open</th>
      <th>Github</th>
      <th>Linkedin</th>
      <th>Ga</th>
      <th>Other</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.name %></td>
        <td><%= profile.title %></td>
        <td><%= profile.user %></td>
        <td><%= profile.skill %></td>
        <td><%= profile.open %></td>
        <td><%= profile.github %></td>
        <td><%= profile.linkedin %></td>
        <td><%= profile.ga %></td>
        <td><%= profile.other %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
